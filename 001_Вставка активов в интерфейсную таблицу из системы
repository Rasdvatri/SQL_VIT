  SELECT ORGANIZATION_CODE,
         INSTANCE_NUMBER,
         INSTANCE_DESCRIPTION,
         PARENT_NUMBER,
         t.ATTRIBUTE7,
         0                       ASSET_FROUP_CODE,
         AREA_CODE,
         SEL1.PER_ORG_CODE,
         SQUAD_CODE,
         t.ATTRIBUTE4,
         t.ATTRIBUTE5,
         t.ATTRIBUTE6,
         t.ATTRIBUTE8,
         t.ATTRIBUTE9,
         t.ATTRIBUTE10,
         t.ATTRIBUTE11,
         t.ATTRIBUTE12,
         t.ATTRIBUTE13,
         t.ATTRIBUTE14,
         t.ATTRIBUTE15,
         t.ATTRIBUTE16,
         t.ATTRIBUTE17,
         t.ATTRIBUTE18,
         t.ATTRIBUTE19,
         t.ATTRIBUTE20,
         t.GROUP_IZNOS,
         t.ASSET_TYPE,  
         MAINTAINABLE_FLAG,
         ATTR_ATTRIBUTE_CATEGORY,
         C_ATTRIBUTE1,
         C_ATTRIBUTE2,
         C_ATTRIBUTE3,
         C_ATTRIBUTE4,
         C_ATTRIBUTE5,
         C_ATTRIBUTE6,
         C_ATTRIBUTE7,
         C_ATTRIBUTE8,
         C_ATTRIBUTE9,
         C_ATTRIBUTE10,
         C_ATTRIBUTE11,
         C_ATTRIBUTE12,
         C_ATTRIBUTE13,
         C_ATTRIBUTE14,
         C_ATTRIBUTE15,
         C_ATTRIBUTE16,
         C_ATTRIBUTE17,
         C_ATTRIBUTE18,
         C_ATTRIBUTE19,
         D_ATTRIBUTE6,
         D_ATTRIBUTE7,
         D_ATTRIBUTE8,
         D_ATTRIBUTE9,
         D_ATTRIBUTE10,
         N_ATTRIBUTE1,
         N_ATTRIBUTE2,
         N_ATTRIBUTE3,
         N_ATTRIBUTE4,
         N_ATTRIBUTE5,
         N_ATTRIBUTE6,
         N_ATTRIBUTE7,
         N_ATTRIBUTE8,
         N_ATTRIBUTE9,
         N_ATTRIBUTE10,
         t.ATTRIBUTE1,
         t.ATTRIBUTE2,
         t.ATTRIBUTE3,
         NULL,
         NULL                    LOAD_STATUS,
         NULL                    ERROR_TEXT,
         t.instance_id,
         NULL
    FROM XXWR_518_INSTANCE_V t,
         (select b.DEPARTMENT_ID, ood.ORGANIZATION_CODE PER_ORG_CODE
            from org_organization_definitions ood, bom_departments_v b
           where ood.ORGANIZATION_ID = b.ORGANIZATION_ID) SEL1
   WHERE to_number(t.attribute12) = SEL1.DEPARTMENT_ID(+)
     and t.organization_code = 'Ð²11'
     and t.maintainable_flag = 'Y'
--     and sel1.per_org_code = '717'
     and t.instance_description like '%111%'
   order by t.instance_number ASC
